{
  "manifest_version": 3,
  "name": "Wikipedia Philosophy Number",
  "version": "0.1",
  "description": "Computes the number of first-link hops to Philosophy.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://en.wikipedia.org/wiki/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
  "16": "images/icon-16.png",
  "48": "images/icon-48.png",
  "128": "images/icon-128.png"
}
}